<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
		<shape>
		    <solid
		        android:color="#C8C8C8" />
		    <stroke
		        android:width="5dp"
		        android:color="#EBEBEB" />
		    <padding
		        android:left="10dp"
		        android:top="10dp"
		        android:right="10dp"
		        android:bottom="10dp" />            
		</shape>  
		
	 </item>
	    <item>
	    <shape>
	        <gradient
	            android:startColor="#D6D6D6"
	            android:endColor="#D6D6D6"
	            android:angle="270" />
	        <stroke
	            android:width="5dp"
	            android:color="#EBEBEB" />
	        <padding
	            android:left="10dp"
	            android:top="10dp"
	            android:right="10dp"
	            android:bottom="10dp" />
	    </shape>
	</item>
</selector>
